{
  "name": "chat-read",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "check-types": "pnpm -r check-types",
    "dev:native": "pnpm --filter native dev",
    "dev:web": "pnpm --filter web dev",
    "check": "oxlint && oxfmt --write"
  },
  "dependencies": {
    "@chat-read/env": "workspace:*",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@chat-read/config": "workspace:*",
    "@types/node": "^22.13.14",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.28.0"
}
